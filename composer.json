{
    "name": "statamic/statamic",
    "description": "Statamic",
    "keywords": [
        "statamic",
        "cms",
        "flat file",
        "laravel"
    ],
    "type": "project",
    "require": {
        "php": "^7.4",
        "edalzell/blade-directives": "^2.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "predis/predis": "^1.1",
        "rias/statamic-button-box": "2.0.2",
        "rias/statamic-color-swatches": "^2.0",
        "rias/statamic-data-import": "dev-master",
        "rias/statamic-link-it": "dev-master",
        "rias/statamic-markdown-highlight": "^1.1.0",
        "rias/statamic-position-fieldtype": "2.0.1",
        "rias/statamic-redirect": "dev-master",
        "spatie/laravel-sitemap": "^5.5",
        "spatie/statamic-responsive-images": "dev-master",
        "statamic/cms": "v3.0.38",
        "statamic/ssg": "^0.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.13",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.2",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan statamic:install --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "build": [
            "yarn production",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php please ssg:generate",
            "@php artisan sitemap"
        ]
    },
    "repositories": []
}
